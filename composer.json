{
  "name": "acelords/laravel-model-ts-type",
  "type": "library",
  "homepage": "https://github.com/acelords/laravel-model-ts-type",
  "description": "This package makes it possible to generate TypeScript types based on your models",
  "keywords": [
    "command",
    "generate",
    "model",
    "TypeScript",
    "type"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rico Clark",
      "email": "rico@scrumble.nl"
    },
    {
      "name": "Luuk de Weijer",
      "email": "luuk@scrumble.nl"
    },
    {
      "name": "SyntaxLexx",
      "email": "syntaxlexx@gmail.com"
    }
  ],
  "autoload": {
    "files": [
      "src/Helpers/helpers.php"
    ],
    "psr-4": {
      "Scrumble\\TypeGenerator\\": "src/"
    },
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": []
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "laravel/framework": "^10.0|^11.0",
    "laravel/helpers": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "larastan/larastan": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "replace": {},
  "extra": {
    "laravel": {
      "providers": [
        "Scrumble\\TypeGenerator\\ServiceProvider"
      ],
      "aliases": {
        "FormatNamespace": "Scrumble\\TypeGenerator\\Facades\\FormatNamespace"
      }
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  }
}
